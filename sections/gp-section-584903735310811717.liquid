    <script>
document.addEventListener('DOMContentLoaded', function() {
  // Find the GemPages product form
  const form = document.querySelector('form[action*="/cart/add"]');
  if (form && window.location.pathname.includes('/products/')) {
    // Get the product handle from URL
    const productHandle = window.location.pathname.split('/products/')[1].split('?')[0];
    
    // Fetch product data to get selling plan
    fetch(`/products/${productHandle}.js`)
      .then(response => response.json())
      .then(product => {
        if (product.variants && product.variants[0] && product.variants[0].selling_plan_allocations && product.variants[0].selling_plan_allocations.length > 0) {
          const planId = product.variants[0].selling_plan_allocations[0].selling_plan.id;
          
          // Add hidden input if it doesn't exist
          let planInput = form.querySelector('input[name="selling_plan"]');
          if (!planInput) {
            planInput = document.createElement('input');
            planInput.type = 'hidden';
            planInput.name = 'selling_plan';
            form.appendChild(planInput);
          }
          planInput.value = planId;
        }
      });
  }
});
</script>
