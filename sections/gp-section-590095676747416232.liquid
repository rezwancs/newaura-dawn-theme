<style>
  /* Scoped styles */
  .na-comment-wrap { max-width: 720px; margin: 0; }
  .na-comment-title { margin: 0 0 10px; font-weight: 600; font-size: 18px; }
  .na-comment-form { display: block; }
  .na-comment-input {
    width: 100%; box-sizing: border-box; resize: vertical;
    padding: 12px 14px; line-height: 1.4; font-size: 14px;
    border: 1px solid #D1D5DB; border-radius: 8px; color: #111827;
    background: #fff; outline: none; transition: border-color .15s, box-shadow .15s;
  }
  .na-comment-input::placeholder { color: #9CA3AF; }
  .na-comment-input:focus {
    border-color: #111827;
    box-shadow: 0 0 0 3px rgba(17,24,39,.08);
  }
  .na-comment-actions {
    margin-top: 8px; display: flex; align-items: center; justify-content: space-between;
  }
  .na-comment-hint { color: #6B7280; font-size: 12px; }
  .na-count { background: #F3F4F6; padding: 2px 6px; border-radius: 999px; }
  .na-comment-btn {
    appearance: none; border: none; cursor: pointer;
    padding: 8px 14px; border-radius: 8px; font-size: 14px; font-weight: 600;
    background: #111827; color: #fff; transition: opacity .15s, transform .02s;
  }
  .na-comment-btn[disabled] { opacity: .45; cursor: not-allowed; }
  .na-comment-btn:active { transform: translateY(1px); }
  .na-comment-toast {
    margin-top: 10px; font-size: 13px; color: #065f46;
