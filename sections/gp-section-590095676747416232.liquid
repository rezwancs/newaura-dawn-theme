<script>
  (function () {
    const ta = document.getElementById('na-comment-input');
    const btn = document.querySelector('.na-comment-btn');
    const toast = document.querySelector('.na-comment-toast');
    const countEl = document.getElementById('na-count');

    // Enable button only when there's non-whitespace text
    ta.addEventListener('input', () => {
      const len = ta.value.trim().length;
      countEl.textContent = ta.value.length;
      btn.disabled = len === 0;
      toast.hidden = true;
    });

    // Fake submit (no network). Show a friendly notice.
    document.querySelector('.na-comment-form').addEventListener('submit', (e) => {
      e.preventDefault();
      btn.disabled = true;
