        "gg-0UyiYZ0g_text": "<p><span style=\"background-color:rgb(255,255,255);color:rgb(48,48,48);font-size:17px;\">Soothes without stripping your hair or moisture.</span></p>",
        "ggBxIwdBfbg_text": "<strong>New Aura™ Flake-Free Cream</strong>",
