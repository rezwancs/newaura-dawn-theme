        "ggdXVGRQywa_text": "FAQs",
        "gghcTBsRB-M_childItem_0": "<p>How fast does it work?</p>",
        "gghcTBsRB-M_childItem_1": "<p>How long do the results last?</p>",
        "gghcTBsRB-M_childItem_2": "<p>Can I use it on my face and scalp?</p>",
